    "ano = int(input('Digite o ano que você quer analisar: '))\n",
    "if ano == 0:\n",
    "    ano = date.today().year\n",
    "if ano %4 == 0 and ano % 100 != 0 or ano % 400 == 0:\n",
    "    print(f'O ano {ano} é Bissexto')\n",
    "else: print(f'O ano {ano} não é bissexto')\n"
